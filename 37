#include <iostream>
#include <cmath>
#include <string>
using namespace std;
bool prime(int n) {
    if (n < 2) return false;
    for (int i = 2; i <= sqrt(n); i++)
        if (n % i == 0) return false;
    return true;
}
bool left(int n) {
    string s = to_string(n);
    while (s.size() > 0) {
        if (!prime(stoi(s))) return false;
        s = s.substr(1); 
    }
    return true;
}

bool right(int n) {
    string s = to_string(n);
    while (s.size() > 0) {
        if (!prime(stoi(s))) return false;
        s = s.substr(0, s.size() - 1); 
    }
    return true;
}

int main() {
    int count = 0;
    long long sum = 0;
    for (int n = 11; count < 11; n++) { 
        if (prime(n) && left(n) && right(n)) {
            sum += n;
            count++;
        }
    }
    cout << sum << endl;
    return 0;
}
