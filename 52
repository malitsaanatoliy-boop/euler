#include <iostream>
#include <string>
#include <algorithm>
using namespace std;

string sorted(long long n){//функция сортировки в строке
    string s = to_string(n);
    sort(s.begin(), s.end());
    return s;
}

int main(){
    for (long long x = 1; ; ++x) {//перебор чисел
        string base = sorted(x);
        bool ok = true;

        for (int k = 2; k <= 6; ++k) {//проверка равенства сортированных 1x, 2x, 3x...
            if (sorted(k * x) != base) {
                ok = false;
                break;
            }
        }

        if (ok) {
            cout << x << endl;
            return 0;
        }
    }
}
