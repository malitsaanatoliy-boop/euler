#include <iostream>
using namespace std;
int main() {
    int ones = 36;      
    int teens = 70;     
    int tens = 46;      
    int one_to_99 = ones + teens + tens*10 + ones*8; 

    int hundreds_base = (ones + 9*7) * 100;
    int ands = 99 * 3 * 9;                  
    int hundreds = hundreds_base + ands + one_to_99 * 9; 

    int sum = one_to_99 + hundreds + 11; 
    cout << sum << endl; 
    return 0;
}