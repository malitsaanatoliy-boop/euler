#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int count = 0;
    int n = 1;

    while (count < 10001) {
        n++;
        bool x = true;
        for (int i = 2; i <= sqrt(n); i++) {
            if (n % i == 0) {
                x = false;
                break;
            }
        }
        if (x) count++;
    }

    cout << n << endl; 
    return 0;
}