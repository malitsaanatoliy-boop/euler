#include <iostream>
using namespace std;

int main() {
    int max = 0;

    for (int i = 100; i <= 999; i++) {
        for (int j = 100; j <= 999; j++) {
            int k = i * j;
            int n = k;
            int reversed = 0;
            while (n > 0) {
                
                reversed = reversed * 10 + n % 10;
                n /= 10;
            }
            if (k == reversed) {
                if (k > max)
                    max = k;
            }
        }
    }

    cout<< max << endl;
    return 0;
}