#include <iostream>
using namespace std;

int primeD(int n) {
    int count = 0;
    for (int i = 2; i*i <= n; ++i) {
        if (n % i == 0) {
            count++;
            while (n % i == 0) n /= i;
        }
    }
    if (n > 1) count++;
    return count;
}

int main() {
    int num = 0;
    int n = 2;
    while (true) {
        if (primeD(n) == 4) {
            num++;
            if (num == 4) {
                cout << n - 3 << endl;
                break;
            }
        } else num = 0;
        n++;
    }
    return 0;
}