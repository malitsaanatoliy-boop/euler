#include <iostream>
using namespace std;
//функция рассчета кол-ва различных простых делителей
int primeD(int n) {
    int count = 0;
    for (int i = 2; i*i <= n; ++i) {
        if (n % i == 0) {
            count++;
            while (n % i == 0) n /= i;
        }
    }
    if (n > 1) count++;
    return count;
}

int main() {
    int num = 0;//счетчик
    int n = 2;//проверяем начиная с 2
    while (true) {
        //если в n ровно 4 различных простых делителя
        if (primeD(n) == 4) {
            num++;//+1 к последовательности
            //если 4 подряд числа то выводим первое из них
            if (num == 4) {
                cout << n - 3 << endl;
                break;
            }
        } else num = 0;//если нет то обнуляем и проверяем следующее
        n++;
    }
    return 0;
}