#include <iostream>
#include <cmath>
using namespace std;
bool x(int n) {
    if (n < 2) return false;
    for (int i = 2; i <= sqrt(n); i++) {
        if (n % i == 0) return false;
    }
    return true;
}

int main() {
    const int N = 2000000;
    long long sum = 0;

    for (int i = 2; i < N; i++) {
        if (x(i)) sum += i;
    }

    cout << sum << endl; 
return 0;
}