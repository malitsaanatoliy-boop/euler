#include <iostream>
using namespace std;
//сумма делителей числа
int sumD(int n) {
    int sum = 1; 
    for (int i = 2; i * i <= n; i++) {
        if (n % i == 0) {
            sum += i;
            if (i != n / i) 
                sum += n / i;
        }
    }
    return sum;
}

int main() {
    int sum = 0;
//проверяем условие дружественных чисел
    for (int a = 2; a < 10000; a++) {
        int b = sumD(a);
        if (b != a && b < 10000 && sumD(b) == a)
            sum += a;
    }

    cout << sum << endl; 
    return 0;
}